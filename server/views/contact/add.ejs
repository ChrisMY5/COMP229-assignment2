<%- include('../components/head.ejs'); %>

<%- include('../components/header.ejs'); %>

<!-- Contents for the Add page-->
<!-- Final view page of data adding -->
<div class="container"><br>
    <div class="row">
        <div class="col-md-5 offset-md-3">
            <h1><%= title %></h1>
            <form class="form" method="post">
                <div class="form-group">
                    <label for="nameTextField">Name</label>
                    <input type="text" class="form-control" 
                    id="nameTextField"
                    placeholder="Enter the Full Name"
                    name="name" required>
                </div>
                <br>
                <div class="form-group">
                    <label for="numberTextField">Phone Number</label>
                    <input type="text" class="form-control" 
                    id="numberTextField"
                    placeholder="Enter the Contact Phone Number"
                    name="number" required>
                </div>
                <br>
                <div class="form-group">
                    <label for="emailTextField">Email Address</label>
                    <input type="text" class="form-control" 
                    id="emailTextField"
                    placeholder="Enter the Email Address"
                    name="email" required>
                </div>
                <br>
                <button class="btn btn-primary">
                    <i class="fas fa-edit"></i>   Submit</button>
                    <a href="/business-contact" class="btn btn-warning">
                    <i class="fas fa-undo"></i> Cancel</a>
            </form>
        </div>
    </div>
</div>




<%- include('../components/footer.ejs'); %>


<%- include('../components/script.ejs'); %>